{
  "name": "Bamboo-Get-Agent",
  "integrationType": "custom-rest-actions",
  "actionType": "custom",
  "config": {
    "request": {
      "requestUrlTemplate": "https://api.bamboohr.com/api/gateway.php/genesystest/v1/employees/directory",
      "requestType": "GET",
      "headers": {
        "Accept": "application/json"
      },
      "requestTemplate": "${input.rawRequest}"
    },
    "response": {
      "translationMap": {},
      "translationMapDefaults": {},
      "successTemplate": "${rawResult}"
    }
  },
  "contract": {
    "input": {
      "inputSchema": {
        "type": "object",
        "properties": {},
        "additionalProperties": true
      }
    },
    "output": {
      "successSchema": {
        "title": "GetEmployeeResponse",
        "type": "object",
        "properties": {
          "employees": {
            "type": "array",
            "items": {
              "title": "Employee",
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "workEmail": {
                  "type": "string"
                },
                "firstName": {
                  "type": "string"
                },
                "lastName": {
                  "type": "string"
                }
              },
              "additionalProperties": true
            }
          }
        },
        "additionalProperties": true
      }
    }
  },
  "secure": false
}